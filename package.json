{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test:vitest": "vitest",
    "test:unit": "run-p test:vitest test:component",
    "test:e2e": "cypress open --e2e -b chrome",
    "test:component": "cypress open --component -b chrome",
    "test:vitest:ci": "vitest --run",
    "test:component:ci": "cypress run --component",
    "test:e2e:ci": "cypress run --e2e",
    "test:unit:ci": "run-p test:vitest:ci test:component:ci",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "prepare": "husky install",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ls-lint": "ls-lint"
  },
  "dependencies": {
    "@unocss/reset": "^0.50.3",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.15",
    "jsdom": "^21.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-demi": "^0.13.11",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.16",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@ls-lint/ls-lint": "^1.11.2",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog-cli": "^2.2.2",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.35.0",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.28.0",
    "shiki": "^0.14.1",
    "taze": "^0.9.0",
    "typescript": "^4.9.5",
    "unocss": "^0.50.3",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.4",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.2.0"
  },
  "engines": {
    "node": ">= 14.16.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,vue}": [
      "eslint --fix",
      "pnpm ls-lint",
      "git add "
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
